{
  "name": "nuzlocke-tracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "babel ./src -d build",
    "start": "node ./build/bin/www",
    "startdev": "nodemon --verbose --exec babel-node ./src/bin/www",
    "lint": "npx eslint ./src",
    "pretty": "prettier --write .",
    "postpretty": "npm run lint --fix",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha -r @babel/register -r test/hooks.js",
    "runQuery": "babel-node ./src/utils/runQuery.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg-monitor": "^1.4.1",
    "pg-promise": "^10.11.1",
    "prettier": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.8",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "supertest": "^6.2.2"
  }
}
